{
  "name": "fisker-blog",
  "version": "1.0.0",
  "description": "fisker's blog",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:gulp": "set NODE_ENV=production && gulp build",
    "build:template": "sh ./build/build-template.sh",
    "dev": "npm run build-template && gulp",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "format:stylelint": "yarn lint:stylelint --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "lint:stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "release": "sh ./build/publish.sh",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/blog.git"
  },
  "keywords": [
    "fisker",
    "blog"
  ],
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fisker/blog/issues"
  },
  "homepage": "https://github.com/fisker/blog#readme",
  "dependencies": {
    "es6-promise": "4.2.8",
    "highlight.js": "9.16.2",
    "localforage": "1.7.3",
    "marked": "0.7.0",
    "primer-markdown": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.5",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.6",
    "@fisker/prettier-config": "1.0.22",
    "@fisker/stylelint-config": "1.1.17",
    "babel-preset-minify": "0.5.1",
    "browser-sync": "2.26.7",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.7.1",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "4.2.0",
    "gulp-dest": "0.2.3",
    "gulp-header": "2.0.9",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.2",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-template": "5.0.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "lodash-cli": "4.17.5",
    "markdownlint-cli": "0.19.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "stylelint": "12.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false
}
